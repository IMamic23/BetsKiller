                    <div class="table-responsive">
                        <table class="table table-hover table-striped" id="reboundsleaders">
                            <thead>
                                <tr>
                                    <th class="text-center">Rank</th>
                                    <th class="text-center">Player</th>
                                    <th class="text-center">Team</th>
                                    <th class="text-center">Double Doubles</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (BetsKiller.ViewModel.Dashboard.Leaders.LeadersCategoryPlayerViewModel leader in Model.DoubleDoubles)
                                {
                                    <tr>
                                        <td class="text-center">@leader.Rank</td>
                                        <td class="text-center">@leader.Player</td>
                                        <td class="text-center"><span class="label label-sm label-@leader.Team.ToLower()">@leader.Team</span></td>
                                        <td class="text-center">@leader.Value</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </section>

    </div>

</section>

@section Scripts {

    @Scripts.Render("~/bundles/dashboard-js")

    <script>
        // Make the dashboard widgets sortable using jQuery UI
        "use strict";
        $(".connectedSortable").sortable({
            placeholder: "sort-highlight",
            connectWith: ".connectedSortable",
            handle: ".box-header, .nav-tabs",
            forcePlaceholderSize: true,
            zIndex: 999999
        });
        $(".connectedSortable .box-header, .connectedSortable .nav-tabs-custom").css("cursor", "move");

    </script>

}
