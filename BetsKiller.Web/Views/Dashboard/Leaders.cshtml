                        <button class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove"><i class="fa fa-times"></i></button>
                    </div>
                </div>
                <div class="box-body">
                    <div class="table-responsive">
                        <table class="table table-hover table-striped" id="pointsleaders">
                            <thead>
                                <tr>
                                    <th class="text-center">Rank</th>
                                    <th class="text-center">Player</th>
                                    <th class="text-center">Team</th>
                                    <th class="text-center">Points</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (BetsKiller.ViewModel.Dashboard.Leaders.LeadersCategoryPlayerViewModel leader in Model.PointsPerGame)
                                {
                                    <tr>
                                        <td class="text-center">@leader.Rank</td>
