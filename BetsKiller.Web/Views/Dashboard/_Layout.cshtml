    <script>

        function getParameterByName(name, url) {
            if (!url) url = window.location.href;
            name = name.replace(/[\[\]]/g, "\\$&");
            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)", "i"),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, " "));
        }

        var subscribe = getParameterByName('subscribe');
        if (subscribe != null && subscribe) {
            $('#subscribeModal').modal('show');
        }

        $('#showProfilePreview').click(function () {
            var url = $('#showProfilePreview').data('url');

            $.get(url, function (data) {
                $('#profileModal').html(data);

                $('#profileModal').modal('show');
            });
        });

        $('#changePasswordBtn').click(function () {
            var url = $('#changePasswordBtn').data('url');

            $.post(url, {
                OldPassword: $('#changePasswordOldPassword').val(),
                NewPassword: $('#changePasswordNewPassword').val(),
                ConfirmPassword: $('#changePasswordConfirmPassword').val()
            })
            .done(function (response) {
                $('#changePasswordMessage').text(response.message);

                if (response.success) {
                    $('#changePasswordMessage').css('color', 'green');

                    setTimeout(function () {
                        $('#changePasswordModal').modal('hide');
                        $('#changePasswordMessage').text('');
                    }, 2000);

                    $('#changePasswordOldPassword').val('');
                    $('#changePasswordNewPassword').val('');
                    $('#changePasswordConfirmPassword').val('');
                }
                else {
                    $('#changePasswordMessage').css('color', 'red');
                }
            });
        });

    </script>
